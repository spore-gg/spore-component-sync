{
  "name": "@trufflehq/cli",
  "version": "0.2.2",
  "description": "The Truffle Developer Platform CLI",
  "main": "dist/cli.js",
  "bin": {
    "truffle-cli": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "swc ./src --out-dir ./dist -C jsc.experimental.keepImportAssertions=true",
    "watch": "swc ./src --out-dir ./dist -C jsc.experimental.keepImportAssertions=true --watch",
    "format": "eslint --fix --ext .ts,.js src",
    "package:linux": "pkg /tmp/tfl --targets node18-linux-x64 --output bin/linux/truffle-cli",
    "package:macos": "pkg /tmp/tfl --targets node18-macos-x64 --output bin/macos/truffle-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trufflehq/truffle-cli.git"
  },
  "author": "Truffle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trufflehq/truffle-cli/issues"
  },
  "homepage": "https://github.com/trufflehq/truffle-cli#readme",
  "dependencies": {
    "@deno/eszip": "^0.22.0",
    "adm-zip": "^0.5.9",
    "arraybuffer-to-buffer": "^0.0.7",
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "common-tags": "^1.8.2",
    "form-data": "^4.0.0",
    "gitignore-to-glob": "^0.3.0",
    "glob": "^8.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2",
    "p-map": "^5.5.0",
    "prettier": "^2.7.1",
    "prettify": "^0.1.7",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13",
    "truffle-dev-server": "github:trufflehq/truffle-dev-server",
    "tsyringe": "^4.7.0",
    "undici": "^5.8.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@types/adm-zip": "^0",
    "@types/common-tags": "^1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.2",
    "@types/readline-sync": "^1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "pkg": "^5.8.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "clean-stack": "3.0.1",
    "indent-string": "4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.7.0",
    "yarn": "1.22.19"
  },
  "packageManager": "yarn@3.3.0"
}
